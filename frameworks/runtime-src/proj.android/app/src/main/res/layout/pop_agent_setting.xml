<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="wrap_content"
              android:orientation="vertical"
              android:layout_height="wrap_content">

    <LinearLayout
            android:layout_width="@dimen/dp_121"
            android:orientation="vertical"
            android:paddingTop="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:background="@drawable/ic_my_proxy_pop"
            android:layout_height="@dimen/dp_130"
    >

        <TextView
                android:id="@+id/total_money"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:textColor="@color/color_fff"
                android:textSize="@dimen/text_size_14"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="@string/home_agent_total_money"
                android:layout_height="0dp"/>
        <TextView android:layout_width="match_parent"
                  android:background="@color/color_30323E"
                  android:layout_height="1px"/>

        <TextView
                android:id="@+id/register_time"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:textColor="@color/color_fff"
                android:textSize="@dimen/text_size_14"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="@string/home_agent_register_time"
                android:layout_height="0dp"/>
        <TextView android:layout_width="match_parent"
                  android:background="@color/color_30323E"
                  android:layout_height="1px"/>
        <TextView
                android:id="@+id/sub_proxy_tv"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:textColor="@color/color_fff"
                android:textSize="@dimen/text_size_14"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="@string/home_agent_sub_agent"
                android:layout_height="0dp"/>

    </LinearLayout>
</LinearLayout>