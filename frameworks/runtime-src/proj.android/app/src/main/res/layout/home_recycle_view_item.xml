<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_height="wrap_content">

    <ImageView
            android:id="@+id/home_image"
            android:layout_marginTop="@dimen/dp_27"
            android:layout_width="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/ic_my_proxy"
            android:layout_height="wrap_content"/>

    <TextView
            android:id="@+id/home_name"
            android:layout_width="match_parent"
            android:text="@string/partner_home_item_my_proxy"
            android:layout_gravity="center"
            android:gravity="center"
            android:textColor="@color/color_1B1D2A"
            android:textSize="@dimen/text_size_14"
            android:layout_marginTop="@dimen/dp_12"
            android:layout_height="wrap_content"/>
    <TextView
            android:id="@+id/home_name_en"
            android:layout_width="match_parent"
            android:text="@string/partner_home_item_my_proxy_en"
            android:layout_gravity="center"
            android:gravity="center"
            android:textColor="@color/color_A8A8A8"
            android:textSize="@dimen/text_size_10"
            android:layout_marginBottom="@dimen/dp_27"
            android:layout_marginTop="@dimen/dp_9"
            android:layout_height="wrap_content"/>


</LinearLayout>



