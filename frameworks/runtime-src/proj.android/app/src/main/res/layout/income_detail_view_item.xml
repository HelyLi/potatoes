<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:layout_height="122dp">

    <TextView
            android:id="@+id/income_time"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:paddingLeft="15dp"
            android:gravity="center_vertical"
            android:paddingRight="15dp"
            style="@style/Theme.Partner.Text.dark.16"
            android:layout_height="0dp"/>

    <TextView
            android:layout_width="match_parent"
            android:background="@color/color_e9e9e9"
            android:layout_height="1px"/>

    <TextView
            android:id="@+id/income_agent_money"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:paddingLeft="15dp"
            android:gravity="center_vertical"
            android:textSize="@dimen/text_size_14"
            android:background="@drawable/bg_white_and_gray_selector"
            android:paddingRight="15dp"
            android:layout_height="0dp"/>

    <TextView
            android:layout_width="match_parent"
            android:background="@color/color_e9e9e9"
            android:layout_marginLeft="15dp"
            android:layout_height="1px"/>

    <TextView
            android:id="@+id/income_member_money"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:background="@drawable/bg_white_and_gray_selector"
            android:textSize="@dimen/text_size_14"
            android:paddingRight="15dp"
            android:layout_height="0dp"/>


</LinearLayout>



